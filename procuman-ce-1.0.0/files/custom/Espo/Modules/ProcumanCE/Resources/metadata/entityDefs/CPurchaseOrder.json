{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "supplier": {
            "type": "link",
            "required": true,
            "audited": true
        },
        "requiredDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "poStatus": {
            "type": "enum",
            "options": [
                "1",
                "2",
                "3",
                "4",
                "5",
                "6",
                "7",
                "8",
                "9"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null,
                "4": null,
                "5": null,
                "6": null,
                "7": null,
                "8": null,
                "9": null
            },
            "default": "1",
            "maxLength": 100,
            "isCustom": true
        },
        "paymentTerms": {
            "type": "multiEnum",
            "storeArrayValues": true,
            "default": [],
            "allowCustomOptions": true,
            "maxCount": 1,
            "options": [
                "1",
                "2",
                "3",
                "4",
                "5"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null,
                "4": null,
                "5": null
            },
            "isCustom": true
        },
        "supplierReference": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "incoTerms": {
            "type": "enum",
            "options": [
                "1",
                "2",
                "3",
                "4",
                "5"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null,
                "4": null,
                "5": null
            },
            "default": "1",
            "maxLength": 100,
            "isCustom": true
        },
        "requiredDocuments": {
            "type": "multiEnum",
            "storeArrayValues": true,
            "default": [],
            "displayAsLabel": true,
            "displayAsList": true,
            "options": [
                "1",
                "2",
                "3",
                "4",
                "5"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null,
                "4": null,
                "5": null
            },
            "allowCustomOptions": true,
            "isCustom": true
        },
        "invoiceReceived": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "goodsReceived": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "internalNotes": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "documents": {
            "type": "linkMultiple",
            "createButton": true,
            "audited": true,
            "tooltip": true
        },
        "poNumber": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 5,
            "prefix": "PO",
            "inlineEditDisabled": true,
            "isCustom": true
        },
        "invoicePaid": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "purchaseOrderLines": {
            "type": "linkMultiple",
            "required": false,
            "audited": true,
            "createButton": true
        },
        "totalAmount": {
            "notNull": false,
            "type": "float",
            "decimalPlaces": 2,
            "audited": true,
            "readOnly": true,
            "isCustom": true
        },
        "taxAmount": {
            "notNull": false,
            "type": "float",
            "decimalPlaces": 2,
            "readOnly": true,
            "audited": true,
            "isCustom": true
        },
        "grandTotal": {
            "notNull": false,
            "type": "float",
            "readOnly": true,
            "audited": true,
            "isCustom": true,
            "decimalPlaces": 2
        },
        "currency": {
            "type": "enum",
            "options": [
                "1",
                "2"
            ],
            "style": {
                "1": null,
                "2": null
            },
            "default": "1",
            "maxLength": 100,
            "isCustom": true
        },
        "shipVia": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "orderDate": {
            "notNull": false,
            "type": "date",
            "default": "javascript: return this.dateTime.getToday();",
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "supplier": {
            "type": "belongsTo",
            "foreign": "purchaseOrders",
            "entity": "CSupplier",
            "audited": false,
            "isCustom": true
        },
        "documents": {
            "type": "hasMany",
            "foreign": "cPurchaseOrder",
            "entity": "Document",
            "audited": false,
            "isCustom": true
        },
        "purchaseOrderLines": {
            "type": "hasMany",
            "foreign": "purchaseOrder",
            "entity": "CPurchaseOrderLine",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}